{
    "name": "egeatech/laravel-responses",
    "description": "A simple package to better handle application responses.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Guidolin",
            "email": "m.guidolin@egeatech.com",
            "homepage": "https://www.egeatech.com"
        }
    ],
    "homepage": "https://github.com/EgeatechSRL/laravel-responses",
    "keywords": ["Laravel", "Responses"],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "~7|~8",
        "egeatech/laravel-exceptions": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "EgeaTech\\LaravelResponses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EgeaTech\\LaravelResponses\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EgeaTech\\LaravelResponses\\Providers\\LaravelResponsesServiceProvider"
            ],
            "aliases": {
                "LaravelResponses": "EgeaTech\\LaravelResponses\\Facades\\LaravelResponses"
            }
        }
    }
}
